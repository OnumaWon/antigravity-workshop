generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Expense {
  id          Int      @id @default(autoincrement())
  description String
  amount      Decimal
  category    String
  createdAt   DateTime @default(now())
}
